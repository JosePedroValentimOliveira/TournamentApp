<%-contentFor('title')%>
<title>Dashboard</title>
<%-contentFor('css')%>


<%-contentFor('script')%>
<script> let events = <%-events-%>;


let div = document.getElementById("events");
events.forEach(event => {
   
    let eventPageLink = document.createElement("a");
   
    let signupLink = document.createElement("a");

    eventPageLink.href = `${event.signupLink}`;
    eventPageLink.textContent = "EventPage";
    

    signupLink.href = `/signup/${event.eventCode}`;
    signupLink.textContent = "SignupPage";
    
    
    div.appendChild(eventPageLink);
    div.appendChild(signupLink);
}); 

</script>



<%-contentFor('body')%>
<button onclick="location.href = '/tournament/new'">New tournament</button>
<button onclick="location.href = '/logout'">Logout</button>
<div id="events">

</div>
<%-contentFor('meta')%>
